#include <iostream>
#include <math.h>

using namespace std;
 
int main() {
    
    double x1, y1, x2, y2, dis;
    
    scanf("%lf""%lf", &x1, &y1);
    scanf("%lf""%lf", &x2, &y2);
    
    dis = pow((pow((x2-x1), 2.0) + pow((y2-y1), 2.0)),1.0/2.0);
    
    printf("%.4lf\n", dis);
    
    return 0;
}
